<script id="question-template" type="text/x-underscore-template">
    <div class="question">
        <div class="panel panel-default">
            <div class="panel-heading">
                <%= name %></h2>
            </div>
            <div class="panel-body">
                <div class="question-body"></div>
            </div>
        </div>
        <h2>
    </div>
</script>
<script id="textField-template" type="text/x-underscore-template">
        <input type="text" id="textField" name="textField" value="<%= value %>" />
</script>
<script id="labelField-template" type="text/x-underscore-template">
        <label for="textField"><%= value %></label>
</script>
<script id="tableLayout-template" type="text/x-underscore-template">
        <table class="tableLayout">
            <tbody>
            <% for(var i = 0; i < rows; i++) {
                    var row = ["<tr>"];
                    for(var j = 0; j < cols; j++){
                        row.push("<td></td>");
                    }
                    row.push("</tr>");
                    print(row.join(""));
                }
             %>
            </tbody>
        </table>
</script>
