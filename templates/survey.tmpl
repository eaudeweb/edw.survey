<script id="question-template" type="text/x-underscore-template">
    <div class="question">
        <div class="panel panel-default">
            <div class="panel-heading">
                <%= name %></h2>
            </div>
            <div class="panel-body">
                <div class="question-body"></div>
            </div>
        </div>
        <h2>
    </div>
</script>
<script id="textField-template" type="text/x-underscore-template">
        <input type="text" id="textField" name="textField" value="<%= value %>" />
</script>
<script id="labelField-template" type="text/x-underscore-template">
        <label for="textField"><%= value %></label>
</script>
