<script id="question-template" type="text/x-underscore-template">
    <div class="question">
        <div class="panel panel-default">
            <div class="panel-body">
                <%= data.name %>
            </div>
        </div>
    </div>
</script>
<script id="splitter-template" type="text/x-underscore-template">
    <li class="splitter">
        <div class="panel panel-default">
            <div class="panel-body">
                <p>SPLITTER</p>
                <ul class="splitter-questions sortable-listing list-unstyled">
                </ul>
                <ul class="splitter-questions sortable-listing new-split">
                </ul>
                <ul class="splitter-questions sortable-listing new-split hidden">
                </ul>
                <div class="clearfix"></div>
            </div>
        </div>
    </li>
</script>
